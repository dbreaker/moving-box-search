<!DOCTYPE html>
<html>
<head>
  <title>MovingBoxSearch.com Admin</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<ul class="hmenu">
	  <%= render 'users/shared/registration_items' %>
	  <%= render 'users/shared/login_items' %>
	</ul>	
	<p id="notice"><%= notice %></p>
	<p id="alert"><%= alert %></p>
	<h1><%= link_to "MovingBoxSearch.com Admin", "/admin" %></h1>
	<ul>
		<li><%= link_to "manage moving box kits", moving_box_kits_path %></li>
		<li><%= link_to "manage move sizes", move_sizes_path %></li>
		<li><%= link_to "manage companies", companies_path %></li>
	</ul>

<%= yield %>

</body>
</html>
